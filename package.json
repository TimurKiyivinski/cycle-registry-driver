{
  "name": "cycle-registry-driver",
  "version": "1.0.4",
  "description": "Docker Registry driver for Cycle.js",
  "main": "dist/makeRegistryDriver.js",
  "scripts": {
    "lint": "standard ./src/*.js",
    "test": "ava",
    "test:watch": "ava --verbose --watch",
    "dist": "babel src -d dist --presets=es2015,babili --no-babelrc",
    "registry": "sudo docker run -d -p 5000:5000 --name registry registry:2",
    "prepublish": "npm run dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TimurKiyivinski/cycle-registry-driver.git"
  },
  "keywords": [
    "docker",
    "registry",
    "cycle",
    "driver",
    "xstream"
  ],
  "author": "Timur Kiyivinski",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/TimurKiyivinski/cycle-registry-driver/issues"
  },
  "homepage": "https://github.com/TimurKiyivinski/cycle-registry-driver#readme",
  "dependencies": {
    "@cycle/http": "^12.0.0",
    "@cycle/run": "^1.0.0",
    "install": "^0.8.7",
    "npm": "^4.2.0",
    "xstream": "^9.3.0"
  },
  "devDependencies": {
    "ava": "^0.18.2",
    "babel-cli": "^6.23.0",
    "babel-preset-babili": "0.0.11",
    "babel-preset-es2015": "^6.22.0",
    "standard": "^8.6.0"
  },
  "ava": {
    "require": ["babel-register"]
  },
  "babel": {
    "presets": ["@ava/stage-4"]
  }
}
